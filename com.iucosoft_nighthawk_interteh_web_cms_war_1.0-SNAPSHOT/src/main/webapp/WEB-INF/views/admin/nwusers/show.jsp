<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@ page session="false"%>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>

<div class="container">



    <h1>User Details</h1>

    <br />

    <div class="row">
        <label class="col-sm-2 control-label">ID</label>
        <div class="col-sm-10">${user.idUser}</div>
    </div>

    <div class="row">
        <label class="col-sm-2">Username</label>
        <div class="col-sm-10">${user.username}</div>
    </div>
    <div class="row">
        <label class="col-sm-2">Password</label>
        <div class="col-sm-10">${user.password}</div>
    </div>
    <div class="row">
        <label class="col-sm-2">Role</label>
        <div class="col-sm-10">${user.role.userRoleName}</div>
    </div>
    <div class="row">
        <label class="col-sm-2">Active</label>
        <div class="col-sm-10">${user.active}</div>
    </div>


</div>


